#include <iostream>
#include <vector>
#include <string>
using namespace std;

int main() {
    // Сохраняем список в переменной
    vector<string> li = {"a", "1", "b", "2", "c", "3"};
    
    // Получаем списки с буквами и числами
    vector<string> li_1;
    vector<string> li_2;
    
    for (int i = 0; i < li.size(); i++) {
        if (i % 2 == 0) {
            li_1.push_back(li[i]);
        } else {
            li_2.push_back(li[i]);
        }
    }
    
    // Очищаем исходный список
    li.clear();
    li.shrink_to_fit(); // Освобождаем память
    
    // Выводим полученные списки
    cout << "li_1: ";
    for (const auto& item : li_1) {
        cout << item << " ";
    }
    cout << "\n\n";
    
    cout << "li_2: ";
    for (const auto& item : li_2) {
        cout << item << " ";
    }
    cout << endl;
    
    return 0;
}
